platform :ios, '12.2'

target 'sdk-core-swift' do
  # Comment the next line if you're not using Swift and don't want to use dynamic frameworks
  use_frameworks!

  pod 'sdk-objectmodel-swift', '~> 3.0'
  pod 'secp256k1.swift'

  target 'sdk-core-swiftTests' do

    pod 'sdk-objectmodel-swift', '~> 3.0'
    pod 'secp256k1.swift'
    
    inherit! :search_paths
    # Pods for testing
  end

end
